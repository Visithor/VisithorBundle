{
    "name": "visithor/visithor-bundle",
    "description": "Visithor Bundle for Symfony",
    "keywords": ["visithor", "php", "Symfony", "Ping"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/config": "~2.3",
        "symfony/dependency-injection": "~2.3",
        "symfony/console": "~2.3",
        "symfony/routing": "~2.3",
        "symfony/http-foundation": "~2.3",
        "symfony/security": "~2.3",
        "symfony/framework-bundle": "~2.3",

        "visithor/visithor": "~0.1, >=0.1.6"
    },
    "require-dev": {
        "symfony/security-bundle": "~2.3",
        "symfony/browser-kit": "~2.3",

        "fabpot/php-cs-fixer": "1.4.2",
        "mmoreram/php-formatter": "1.0.4",
        "phpunit/phpunit": "4.5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Visithor\\Bundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
