{
    "name": "visithor/visithor-bundle",
    "description": "Visithor Bundle for Symfony",
    "keywords": ["visithor", "php", "Symfony", "Ping"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morera",
            "email": "yuhu@mmoreram.com"
        }
    ],
    "require": {
        "php": "^5.4|^7.0",
        "symfony/config": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/console": "^2.7|^3.0",
        "symfony/routing": "^2.7|^3.0",
        "symfony/http-foundation": "^2.7|^3.0",
        "symfony/security": "^2.7|^3.0",
        "symfony/framework-bundle": "^2.7|^3.0",

        "visithor/visithor": "^0.2.0"
    },
    "require-dev": {
        "symfony/security-bundle": "^2.3|^3.0",
        "symfony/browser-kit": "^2.3|^3.0",

        "fabpot/php-cs-fixer": "1.4.2",
        "mmoreram/php-formatter": "1.1.0",
        "phpunit/phpunit": "4.5.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Visithor\\Bundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
